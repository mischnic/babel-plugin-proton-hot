{
  "name": "@mischnic/babel-plugin-proton-hot",
  "author": "Niklas Mischkulnig",
  "version": "1.0.1",
  "license": "MIT",
  "description": "Transform proton-native code to be hot reloadable",
  "main": "index.js",
  "scripts": {
    "lint": "prettier --use-tabs --tab-width=4 --print-width=100 --write index.js test/index.js test/units/*/*.js",
    "test": "node test/index.js | tap-spec",
    "coverage": "nyc --include index.js --reporter html --reporter text npm run test"
  },
  "dependencies": {
    "react-proxy": "^1.1.8"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "nyc": "^12.0.2",
    "prettier": "^1.13.7",
    "tap-spec": "^5.0.0"
  },
  "files": [
    "index.js"
  ]
}
